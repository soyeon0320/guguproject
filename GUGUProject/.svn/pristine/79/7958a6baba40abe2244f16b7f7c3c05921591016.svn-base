<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>채용설명회 등록</title>
</head>
<body>
	<form action="/GUGUProject/jobnews/insert.do" method="post" >
		<table>
			<tr>
				<td>구분:</td>
				<td>
					<select name="jobnewsGubun">
						<option value="NEWS">뉴스</option>
						<option value="TIPS">취업팁</option>
						<option value="SISA">시사</option>
					</select>
				
				</td>
			</tr>
			<tr>
				<td>뉴스제목:</td>
				<td><input type="text" name="jobnewsTitle" value=""></td>
			</tr>
			<tr>
				<td>기자명:</td>
				<td><input type="text" name="jobnewsEditor" value=""></td>
			</tr>
			<tr>
				<td>작성일:</td>
				<td><input type="text" name="jobnewsRegDate" value=""></td>
			</tr>
			<tr>
				<td>출처:</td>
				<td><input type="text" name="jobnewsCopyright" value="" ></td>
			</tr>			
			<tr>
				<td>이미지:</td>
				<td><input type="text" name="jobnewsUrl"></td>
			</tr>			
			<tr>
				<td>내용:</td>
				<td><input type="text" name="jobnewsContent"></td>
			</tr>
			
		</table>
		<input type="submit" value="등록">
	
	</form>
</body>
</html>